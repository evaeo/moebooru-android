<resources>

    <string-array name="schema">
        <item>http://</item>
        <item>https://</item>
    </string-array>

    <string-array name="filter_type_1">
        <item>id</item>
        <item>height</item>
        <item>width</item>
    </string-array>

    <string-array name="filter_compare">
        <item>:</item>
        <item>:></item>
        <item>:>=</item>
        <item>:&lt;</item>
        <item>:&lt;=</item>
    </string-array>

    <string-array name="filter_order">
        <item>id</item>
        <item>id_desc</item>
        <item>fav</item>
        <item>wide</item>
        <item>nonwide</item>
        <item>score</item>
        <item>score_asc</item>
        <item>mpixels</item>
        <item>mpixels_asc</item>
        <item>landscape</item>
        <item>portrait</item>
        <item>vote</item>
        <item>favcount</item>
    </string-array>

    <string-array name="filter_rating">
        <item>safe</item>
        <item>questionable</item>
        <item>explicit</item>
    </string-array>

    <integer-array name="customizedColors">
        <item>@color/blue</item>
        <item>@color/purple</item>
        <item>@color/green</item>
        <item>@color/orange</item>
        <item>@color/red</item>
        <item>@color/dark_blue</item>
        <item>@color/dark_purple</item>
        <item>@color/dark_green</item>
        <item>@color/dark_orange</item>
        <item>@color/dark_red</item>
    </integer-array>

    <string-array name="night_modes">
        <item>@string/night_mode_system</item>
        <item>@string/night_mode_auto</item>
        <item>@string/night_mode_off</item>
        <item>@string/night_mode_on</item>
    </string-array>

    <string-array name="night_mode_values">
        <item>system</item>
        <item>auto</item>
        <item>off</item>
        <item>on</item>
    </string-array>

    <string-array name="post_limit">
        <item>40</item>
        <item>@string/post_limit_default</item>
        <item>60</item>
        <item>70</item>
        <item>80</item>
        <item>90</item>
        <item>100</item>
    </string-array>

    <string-array name="post_limit_values">
        <item>40</item>
        <item>50</item>
        <item>60</item>
        <item>70</item>
        <item>80</item>
        <item>90</item>
        <item>100</item>
    </string-array>

    <string-array name="cache_memory">
        <item>@string/cache_memory_default</item>
        <item>256 MiB</item>
        <item>512 MiB</item>
        <item>1 GiB</item>
    </string-array>

    <string-array name="cache_memory_values">
        <item>128</item>
        <item>256</item>
        <item>512</item>
        <item>1024</item>
    </string-array>

    <string-array name="cache_disk">
        <item>128 MiB</item>
        <item>@string/disk_memory_default</item>
        <item>512 MiB</item>
        <item>1 GiB</item>
        <item>2 GiB</item>
    </string-array>

    <string-array name="cache_disk_values">
        <item>128</item>
        <item>256</item>
        <item>512</item>
        <item>1024</item>
        <item>2048</item>
    </string-array>

    <string-array name="post_size">
        <item>@string/post_size_sample</item>
        <item>@string/post_size_larger</item>
        <item>@string/post_size_origin</item>
    </string-array>

    <string-array name="post_size_values">
        <item>sample</item>
        <item>larger</item>
        <item>origin</item>
    </string-array>

</resources>